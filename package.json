{
  "name": "@sammarks/icons",
  "version": "0.0.0-development",
  "description": "My personal componetized SVG Icons for React",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/icons.esm.js",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sammarks/icons.git"
  },
  "files": [
    "dist",
    "native"
  ],
  "scripts": {
    "build": "tsdx build -i src/index.ts -i src/native.tsx && cp dist/index.js native/index.js && mv dist/native.d.ts native/index.d.ts && rm -rf native/src",
    "build:storybook": "build-storybook",
    "format:all": "yarn format:source && yarn format:svgs",
    "format:source": "yarn prettier --write \"./src/**/*.ts*\" \"./src/**/*.js\"",
    "format:svgs": "yarn svgo -f src/svgs --pretty --config=svgo.config.json",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "yarn jest --maxWorkers=2",
    "test:ci": "yarn test --ci=true --coverage=true"
  },
  "storybook-deployer": {
    "commitMessage": "chore: deploy storybook [ci skip]"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-react-display-name": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addons": "^5.3.8",
    "@storybook/react": "^5.3.8",
    "@storybook/storybook-deployer": "^2.8.5",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native-svg": "^12.1.0",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-string": "^3.0.0",
    "semantic-release": "^17.0.4",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  }
}
